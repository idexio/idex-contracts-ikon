{
  "name": "@idexio/idex-contracts-ikon",
  "license": "UNLICENSED",
  "version": "0.1.18",
  "description": "Collects source code, tests, and documentation for the IDEX Ikon release Ethereum contracts",
  "homepage": "https://github.com/idexio/idex-contracts-ikon",
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "directories": {
    "lib": "build/lib"
  },
  "files": [
    "artifacts/contracts/libraries/Deleveraging.sol/Deleveraging.json",
    "artifacts/contracts/libraries/Depositing.sol/Depositing.json",
    "artifacts/contracts/libraries/Funding.sol/Funding.json",
    "artifacts/contracts/libraries/Liquidation.sol/Liquidation.json",
    "artifacts/contracts/libraries/Margin.sol/Margin.json",
    "artifacts/contracts/libraries/MarketAdmin.sol/MarketAdmin.json",
    "artifacts/contracts/libraries/NonceInvalidations.sol/NonceInvalidations.json",
    "artifacts/contracts/libraries/Trading.sol/Trading.json",
    "artifacts/contracts/libraries/Withdrawing.sol/Withdrawing.json",
    "build/lib",
    "build/typechain-types"
  ],
  "scripts": {
    "build": "yarn build:sol && yarn build:ts",
    "build:clean": "rm -rf build && rm -rf artifacts && rm -rf typechain-types",
    "build:sol": "hardhat compile",
    "build:ts": "tsc -p .",
    "test": "hardhat test"
  },
  "repository": "https://github.com/idexio/idex-contracts-ikon",
  "dependencies": {
    "chai": "4.2.0",
    "ethers": "5.6.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "^4.6.0",
    "@typechain/ethers-v5": "10.1.0",
    "@typechain/hardhat": "6.1.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "hardhat": "2.10.2",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^10.1.0",
    "typechain": "8.1.0",
    "typescript": "^4.5.2"
  }
}
